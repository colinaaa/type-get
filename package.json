{
  "name": "type-get",
  "version": "1.0.0",
  "description": "Get property using dot-notated path. (With types!)",
  "source": "src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "keywords": [
    "get",
    "dot notation",
    "dot"
  ],
  "files": [
    "dist"
  ],
  "author": "Colin Wang",
  "repository": "https://github.com/colinaaa/type-get",
  "license": "MIT",
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "yarn pinst --disable",
    "postpublish": "yarn pinst --enable",
    "release": "yarn build && yarn changeset publish",
    "build": "microbundle build",
    "build:nocompress": "yarn build --no-compress",
    "start": "microbundle watch"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.0",
    "@changesets/cli": "^2.16.0",
    "@jest/types": "^27.0.6",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.1",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.0",
    "microbundle": "^0.13.3",
    "pinst": "^2.1.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write"
    ]
  }
}
